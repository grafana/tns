<html op="news">
   <head>
      <meta name="referrer" content="origin">
      <meta name="viewport" content="width=device-width, initial-scale=1.0">
      <link rel="stylesheet" type="text/css" href="https://news.ycombinator.com/news.css">
      <title>Grafana News</title>
   </head>
   <body>
      <center>
         <table id="hnmain" border="0" cellpadding="0" cellspacing="0" width="85%" bgcolor="#f6f6ef">
            <tr>
               <td bgcolor="#ff6600">
                  <table border="0" cellpadding="0" cellspacing="0" width="100%" style="padding:2px">
                     <tr>
                        <td style="width:18px;padding-right:4px"><a href="https://grafana.com"><img src="data:image/png;base64," width="18" height="18"></a></td>
                        <td style="line-height:12pt; height:10px;"><span class="pagetop">
                            <b class="hnname"><a href="news">Grafana News</a></b>
                        </td>
                        <td style="text-align:right;padding-right:4px;"><span class="pagetop">
                        </td>
                     </tr>
                  </table>
               </td>
            </tr>
            <tr id="pagespace" title="" style="height:10px"></tr>
            <tr>
               <td>
                  <table border="0" cellpadding="0" cellspacing="0" class="itemlist">
    	            {{ range .Links }}
                     <tr class='athing' id='{{ .ID }}'>
                        <td align="right" valign="top" class="title"><span class="rank">{{ .Rank }}.</span></td>
                        <td valign="top" class="votelinks">
                           <center>
                              <a id='up_{{ .ID }}' href='vote?id={{ .ID }}&amp;how=up'>
                                 <div class='votearrow' title='upvote'></div>
                              </a>
                           </center>
                        </td>
                        <td class="title"><a href="{{ .URL }}" class="storylink">{{ .Title }}</a></td>
                     </tr>
                     <tr>
                        <td colspan="2"></td>
                        <td class="subtext">
                          <span class="score" id="score_{{ .ID }}">{{ .Points }} points</span>
                        </td>
                     </tr>
                     <tr class="spacer" style="height:5px"></tr>
                  {{ end }}
                  </table>
               </td>
            </tr>
            <tr>
               <td>
                  <table width="100%" cellspacing="0" cellpadding="1">
                     <tr>
                        <td bgcolor="#ff6600"></td>
                     </tr>
                  </table>
                  <br>
                  <center>
                    <form action="/post" method="post">
                        Title: <input name="title" /><br/>
                        URL: <input name="url" /><br/>
                        <button name="submit" value="" type="submit">Submit</button>
                    </form>
                  </center>
               </td>
            </tr>
         </table>
      </center>
   </body>
</html>
